#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int multiplo(int a ,int b);

int main()
{
   int a,b,m;
	cout<<"Ingrese el primer valor";
	cin>>a;
	cout<<"Ingrese el segundo valor";
	cin>>b;
	m=multiplo(a,b);
	cout<<m;
	getch();
	 return 0;
}
int multiplo(int a ,int b){
int m;
	if (a>b){
		if (a%b==0)
			m=1;
	}
	else{
		if (b%a==0)
			m=0;
	}

	return m;
}


